<?xml version="1.0" encoding="utf-8"?>
<com.appslock.fingerprint.samplenotchapp.FullscreenOverlayLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/overlay_service_mainlay"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <com.appslock.fingerprint.samplenotchapp.OverlayDrawingView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/button_notch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Notch"
        android:paddingHorizontal="20dp"
        android:paddingVertical="10dp"
        android:background="@color/light_blue_900"
        android:textColor="@color/white"
        android:layout_gravity="center_horizontal" />

</com.appslock.fingerprint.samplenotchapp.FullscreenOverlayLayout>